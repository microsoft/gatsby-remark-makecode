{
    "name": "gatsby-remark-makecode",
    "version": "1.1.0",
    "peerDependencies": {
        "puppeteer": "^10.2.0",
        "sharp": "^0.30.5"
    },
    "dependencies": {
        "@types/node": "^16.9.1",
        "hast-util-to-html": "^8.0.2",
        "mdast-util-to-hast": "^11.2.1",
        "node-fetch": "^2.0.0",
        "remark": "^14.0.1",
        "unist-util-find": "1.0.2",
        "unist-util-visit": "2.0.3"
    },
    "devDependencies": {
        "@semantic-release/git": "^9.0.1",
        "puppeteer": "^10.2.0",
        "sharp": "^0.30.5",
        "typescript": "^4.4.3"
    },
    "main": "dist/index.js",
    "files": [
        "dist/**"
    ],
    "scripts": {
        "build": "node node_modules/typescript/bin/tsc",
        "watch": "node node_modules/typescript/bin/tsc --watch"
    },
    "engines": {
        "node": ">=12.13.0"
    },
    "release": {
        "branch": "main",
        "branches": [
            "main"
        ],
        "plugins": [
            [
                "@semantic-release/commit-analyzer",
                {
                    "preset": "angular",
                    "releaseRules": [
                        {
                            "type": "doc",
                            "release": "patch"
                        },
                        {
                            "type": "fix",
                            "release": "patch"
                        },
                        {
                            "type": "patch",
                            "release": "patch"
                        },
                        {
                            "type": "minor",
                            "release": "minor"
                        },
                        {
                            "type": "feat",
                            "release": "minor"
                        },
                        {
                            "type": "feature",
                            "release": "minor"
                        },
                        {
                            "scope": "no-release",
                            "release": false
                        }
                    ]
                }
            ],
            [
                "@semantic-release/release-notes-generator"
            ],
            [
                "@semantic-release/github",
                {
                    "successComment": false,
                    "failComment": false
                }
            ],
            [
                "@semantic-release/npm"
            ],
            [
                "@semantic-release/git",
                {
                    "assets": [
                        "package.json"
                    ]
                }
            ]
        ]
    }
}
